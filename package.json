    "html-webpack-plugin": "^2.29.0",
    "lerna": "^2.0.0-rc.5",
    "ts-loader": "^2.2.0",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "2.4.0",
